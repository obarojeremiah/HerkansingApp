<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.realdolmen.bestellingapp">
    <uses-permission android:name="android.permission.VIBRATE"/>

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".model.DetailProduct">
        </activity>
        <activity
                android:name=".ScannerActivity"
                android:screenOrientation="portrait">
        </activity>
        <activity android:name=".InlineScanActivity"></activity>
        <activity android:name=".ProductCard">
        </activity>
        <activity android:name=".OverviewActivity">
        </activity>
        <activity android:name=".OverzichtAankoop">
        </activity>
        <activity android:name=".model.EndOfBuyingItem">
        </activity>
        <activity android:name=".model.Purchase">
        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>